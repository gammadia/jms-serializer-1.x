includes:
    - 'phpstan-baseline.neon'

parameters:
    level: max
    tmpDir: 'var/cache/phpstan/main'
    paths:
        - 'src'
        - 'tests'
    phpVersion: 74000
    reportUnmatchedIgnoredErrors: true
    checkFunctionNameCase: true
    checkUninitializedProperties: true
    tipsOfTheDay: false
